
<div class="row">
 <div class="col-xs-12">
  <h4>
    <%= title %>
  </h4>
</div>
</div>
<div class="row ">
      <div class="col-xs-6">
<table class="table table-condensed">
    <tr>
        <th scope="row">Sehir</th>
        <td><%= evidence.city %></td>
    </tr>
    <tr>
        <th scope="row">Ilce</th>
        <td><%= evidence.district %></td>
    </tr>
     <tr>
        <th scope="row">Sandik Numarasi</th>
        <td><%= evidence.no %></td>
    </tr>
      <tr>
        <th scope="row">Sandik Turu</th>
        <td><%= types.evidence[evidence.type] %></td>
    </tr>
      <tr>
        <th scope="row"></th>
        <td></td>
    </tr>
</table>
   </div>
  <!--   <div class="col-xs-5">
         <a href="/readings/<%= evidence._id %>/new">
           <input type="submit" class="btn btn-default btn-danger btn-lg btn-block" value="FLAG (show da olmali)"/></a>
    </div> -->
</div>

<div class="row ">
          <div class="col-xs-12">
              <img src="<%=s3path + evidence.img %>" width="100%">
          </div>
</div>
<hr>

<form role="form" id="evidence_edit" class="form-horizontal" action="/readings" method="post" accept-charset="utf-8" enctype="multipart/form-data">

 <input type="hidden" id="evidence_id" name="evidence_id" value="<%= evidence._id %>"/>

<div class="row" >
	<div class="col-xs-6">
 		<div class="row">
			<div class="col-xs-12">
	    		<h4>Veriler</h4>
	    	</div>
	    </div>
	   	<div class="row tutanak-form">
 			<div class="col-xs-9 tutanak-label ">
	    		Kayıtlı Seçmen:
	    	</div>
	    	<div class="col-xs-3">
	    		<input type="number" class="form-control input-sm" name="baskan_kayitli_secmen" value=0>
	    	</div>
	    </div>
	    <div class="row tutanak-form">
 			<div class="col-xs-9 tutanak-label">
	    		(1) Oy Kullanan Kayıtlı Seçmen:
	    	</div>
	    	<div class="col-xs-3">
	    		<input type="number" class="form-control input-sm" name="baskan_oy_kullanan_secmen" value=0>
	    	</div>
	    </div>
	    <div class="row tutanak-form">
 			<div class="col-xs-9 tutanak-label">
	    		(2) Kanun Gereği Oy Kullanan:
	    	</div>
	    	<div class="col-xs-3">
	    		<input type="number" class="form-control input-sm" name="baskan_kanunen_oy_kullanan_secmen" value=0>
	    	</div>
	    </div>
	     <div class="row tutanak-form">
 			<div class="col-xs-9 tutanak-label">
	    		(3) Kullanılan Toplam Oy:
	    	</div>
	    	<div class="col-xs-3">
	    		<input type="number" class="form-control input-sm" name="baskan_toplam_oy_kullanan_secmen" value=0>
	    	</div>
	    </div>
	    <div class="row tutanak-form">
 			<div class="col-xs-9 tutanak-label">
	    		(4) Sandiktan Cikan Zarf Sayisi
	    	</div>
	    	<div class="col-xs-3">
	    		<input type="number" class="form-control input-sm" name="baskan_sandiktan_cikan_zarf_sayisi" value=0>
	    	</div>
	    </div>
	    <div class="row tutanak-form">
 			<div class="col-xs-9 tutanak-label">
	    		(6) Gecerli Zarf Sayisi:
	    	</div>
	    	<div class="col-xs-3">
	    		<input type="number" class="form-control input-sm" name="baskan_gecerli_zarf_sayisi" value=0>
	    	</div>
	    </div>
	     <div class="row tutanak-form">
 			<div class="col-xs-9 tutanak-label">
	    		(7) İtirazsız Geçerli Oy:
	    	</div>
	    	<div class="col-xs-3">
	    		<input type="number" class="form-control input-sm" name="baskan_itirazsiz_gecerli_oy" value=0>
	    	</div>
	    </div>
	    <div class="row tutanak-form">
 			<div class="col-xs-9 tutanak-label">
	    		(8) İtirazlı Geçerli Oy:
	    	</div>
	    	<div class="col-xs-3">
	    		<input type="number" class="form-control input-sm" name="baskan_itirazli_gecerli_oy" value=0>
	    	</div>
	    </div>
	    <div class="row tutanak-form">
 			<div class="col-xs-9 tutanak-label">
	    		(9) Geçerli Oy:
	    	</div>
	    	<div class="col-xs-3">
	    		<input type="number" class="form-control input-sm" name="baskan_gecerli_oy" value=0>
	    	</div>
	    </div>
	    <div class="row tutanak-form">
 			<div class="col-xs-9 tutanak-label">
	    		(15) Geçersiz Oy:
	    	</div>
	    	<div class="col-xs-3">
	    		<input type="number" class="form-control input-sm" name="baskan_gecersiz_oy" value=0>
	    	</div>
	    </div>
	     <div class="row tutanak-form">
 			<div class="col-xs-9 tutanak-label">
	    		(15) Toplam Geçerli Oy:
	    	</div>
	    	<div class="col-xs-3">
	    		<input type="number" class="form-control input-sm" name="baskan_toplam_gecerli_oy" value=0>
	    	</div>
	    </div>
	</div>
 	<div class="col-xs-6">
		<div class="row">
			<div class="col-xs-12">
	    		<h4>Adaylar</h4>	
	    	</div>
	    </div>
		<div class="row">
			<div class="col-xs-4">
	    		Partiiii 
	    	</div>
 			<div class="col-xs-5">
	    		Aday Isim
	    	</div>
	    	<div class="col-xs-3">
	    		Oy SAYISI
	    	</div>
	    </div>
	    	<div class="row">
			<div class="col-xs-12">
	    		<hr>
	    	</div>
	    </div>
	     <% for(var k=0; k< candidates.length; k++) {%>
		<div class="row">
			<div class="col-xs-4 tutanak-label">
	    		<%= candidates[k].party %>
	    	</div>
 			<div class="col-xs-5 tutanak-label">
	    		<%= candidates[k].person %>
	    	</div>
	    	<div class="col-xs-3 tutanak-form">
	    		<input type="number" class="form-control input-sm" name="baskan_adaylar[]" value=0>
	    	</div>
	    </div>
	     <% } %>
	</div>
</div>
<div class="row">
  <div class="col-xs-12">
        
           <input type="submit" class="btn btn-default btn-primary btn-lg btn-block" value="EKLE"/>
    </div>
</div>

</form>